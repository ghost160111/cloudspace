{
    "name": "cloudspace-react-ui",
    "private": true,
    "version": "0.0.0",
    "type": "module",
    "scripts": {
        "dev": "vite --host",
        "build": "tsc -b && vite build && node generate-i18n-html.js",
        "preview": "vite preview --host",
        "watch": "vite build --watch",
        "format": "npx prettier --write \"**/*.{html,css,scss,js,ts,jsx,tsx,json}\"",
        "check-format": "prettier --check \"**/*.{html,css,scss,js,ts,jsx,tsx,json}\"",
        "test": "vitest run",
        "test:watch": "vitest",
        "scan": "npx react-scan http://localhost:5173",
        "lint": "npx eslint src/"
    },
    "dependencies": {
        "@fancyapps/ui": "^5.0.36",
        "@tailwindcss/vite": "^4.1.12",
        "echarts": "^5.6.0",
        "flag-icons": "^7.5.0",
        "lodash": "^4.17.21",
        "mobx": "^6.13.7",
        "mobx-react": "^9.2.0",
        "rc-slider": "^11.1.8",
        "react": "^19.1.1",
        "react-dom": "^19.1.1",
        "react-router-dom": "^7.4.1",
        "sass": "^1.86.2",
        "swiper": "^11.2.6",
        "tailwindcss": "^4.1.12",
        "vite-plugin-pwa": "^1.0.3"
    },
    "devDependencies": {
        "@testing-library/jest-dom": "^6.6.4",
        "@testing-library/react": "^16.3.0",
        "@testing-library/user-event": "^14.6.1",
        "@types/echarts": "^4.9.22",
        "@types/lodash": "^4.17.15",
        "@types/node": "^22.13.4",
        "@types/rc-slider": "^9.3.0",
        "@types/react": "^19.1.0",
        "@types/react-dom": "^19.1.1",
        "@types/react-router-dom": "^5.3.3",
        "@types/swiper": "^5.4.3",
        "@types/tailwindcss": "^3.0.11",
        "@typescript-eslint/parser": "^8.42.0",
        "@vitejs/plugin-react-swc": "^3.10.0",
        "eslint": "^9.34.0",
        "jsdom": "^26.1.0",
        "prettier": "^3.6.2",
        "react-scan": "^0.4.3",
        "sharp": "^0.34.3",
        "svgo": "^4.0.0",
        "typescript": "~5.8.3",
        "vite": "^7.1.2",
        "vite-plugin-image-optimizer": "^2.0.2",
        "vitest": "^3.2.4"
    }
}
